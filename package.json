    "start": "tsdx watch"
  },
  "peerDependencies": {
    "react": ">= 16.8.3",
    "react-dom": ">= 16.8.3",
    "styled-components": ">= 4.1.3"
  },
  "dependencies": {
    "react-select": "^3.1.0",
    "@types/react-select": "3.0.13"
  },
  "devDependencies": {
    "@babel/core": "7.10.2",
    "@types/node": "13.13.5",
    "@types/react": "16.9.38",
    "@types/react-dom": "16.9.8",
    "@types/react-select": "3.0.13",
    "@types/styled-components": "5.1.0",
    "@typescript-eslint/eslint-plugin": "3.3.0",
    "@typescript-eslint/parser": "3.3.0",
    "eslint": "7.2.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.20.0",
    "gh-pages": "3.0.0",
    "husky": "4.2.5",
    "prettier": "2.0.5",
    "pretty-quick": "2.0.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "styled-components": "5.1.1",
    "tslib": "2.0.0",
    "typescript": "3.9.5"
  },
  "husky": {
    "hooks": {
