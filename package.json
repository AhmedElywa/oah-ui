    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^5.1.4",
    "gh-pages": "^1.2.0",
    "next": "^7.0.2",
    "prop-types": "^15.6.2",
