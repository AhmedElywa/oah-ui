{
  "name": "@paljs/ui",
  "version": "1.0.0",
  "description": "React Styled Components with bootstrap grid system",
  "main": "dist/index.js",
  "module": "dist/ui.esm.js",
  "types": "dist/index.d.ts",
  "repository": "https://github.com/paljs/ui.git",
  "author": "Ahmed Elywa",
  "license": "MIT",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
