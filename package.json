  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "bugs": {
    "url": "https://github.com/paljs/ui/issues",
    "email": "ahmed.elywa@icloud.com"
  },
  "homepage": "https://github.com/paljs/ui/"
