{
  "name": "oah-ui",
  "version": "0.1.3",
  "description": "React Styled Components with bootstrap grid system",
  "main": "index.js",
  "repository": "https://github.com/oahtech/oah-ui.git",
  "author": "OAH Technology",
  "license": "MIT",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "build": "rollup -c",
    "pro": "cross-env NODE_ENV=production BABEL_ENV=production rollup -c",
    "start": "rollup -c -w",
    "prepare": "yarn run build",
    "deploy": "rollup -c && cd dist && yarn publish"
  },
  "peerDependencies": {
    "react": ">= 16.8.3",
    "react-dom": ">= 16.8.3",
    "styled-components": ">= 4.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-react": "^7.7.4",
    "@svgr/rollup": "^4.3.3",
    "babel-eslint": "^10.0.3",
    "babel-plugin-styled-components": "^1.10.6",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "cross-env": "^6.0.3",
    "eslint": "^6.7.1",
    "eslint-plugin-react-hooks": "^2.3.0",
    "gh-pages": "^2.1.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "^3.1.2",
    "rollup": "^1.27.4",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.1.2",
    "rollup-plugin-url": "^2.2.2",
    "styled-components": "^4.4.1"
  },
  "dependencies": {
    "polished": "^3.4.2",
    "prop-types": "^15.7.2"
  }
}
